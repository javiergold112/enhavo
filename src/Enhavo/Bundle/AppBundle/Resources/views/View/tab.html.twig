{% trans_default_domain translationDomain %}
<div class="row tabContainer">
    <div class="container-fluid">
        {% for tab in tabs %}
          <a href="#" tabId="tab{{ loop.index }}"><span>{{ tab.label|trans|nl2br }}</span></a>
        {% endfor %}
    </div>
</div>

<div class="row tabContentContainer">
    {% for tab in tabs %}
        <div id="tab{{ loop.index }}" class="col-xs-12" data-form-tab>
            {% include tab.template  %}
        </div>
    {% endfor %}
</div>

{% if form._token is defined %}
    {% if not form._token.rendered %}
        {{ form_row(form._token) }}
    {% endif %}
{% endif %}
