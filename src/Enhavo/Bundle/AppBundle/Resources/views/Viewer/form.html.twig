{% extends 'EnhavoAppBundle:Viewer:base.html.twig' %}

{% block content %}
    <div id="app"></div>
    <div data-tab-container style="display: none">
        {% for key,tab in tabs %}
            <div data-tab="{{ key }}">
                {% include tab.template %}
            </div>
        {% endfor %}
    </div>
{% endblock %}