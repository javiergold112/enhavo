<h1>Addressing</h1>
<div>
    <form action="{{ path('sylius_flow_forward', {'scenarioAlias': 'enhavo_checkout', 'stepName': 'addressing'}) }}" method="POST">
        {{ form_errors(form) }}
        {{ form_row(form.billingAddress) }}
        {{ form_row(form.differentBillingAddress) }}
        {{ form_row(form.shippingAddress) }}
        {{ form_row(form._token) }}
        <button type="submit">Senden</button>
    </form>
</div>