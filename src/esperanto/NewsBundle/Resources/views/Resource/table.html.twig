<div class="row head-row">
    <div class="col-md-1">{{ 'label.id'|trans }}</div>
    <div class="col-md-7">{{ 'label.title'|trans }}</div>
    <div class="col-md-4">{{ 'label.publication_date'|trans }}</div>
</div>

{% for item in data %}
    <div class="row entry-row" data-id="{{ item.id }}">
        <div class="col-md-1">{{ item.id }}</div>
        <div class="col-md-7">{{ item.title }}</div>
        <div class="col-md-4">{{ item.publicationDate|date("d.m.Y") }}</div>
    </div>
{% else %}
    {{ 'table.empty'|trans }}
{% endfor %}